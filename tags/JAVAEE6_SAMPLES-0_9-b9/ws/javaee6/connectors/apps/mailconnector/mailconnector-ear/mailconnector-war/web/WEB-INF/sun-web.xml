<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD GlassFish Application Server 3.0 Servlet 3.0//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_3_0-0.dtd">        

<sun-web-app>
    <context-root>mailconnector-war</context-root>
    <session-config>
        <session-manager/>
    </session-config>
    <resource-ref>
        <res-ref-name>eis/MyConnectionFactory</res-ref-name>
        <jndi-name>eis/JavaMailConnectionFactory</jndi-name>
        <default-resource-principal>
          <name>j2ee</name>
          <password>j2ee</password>
        </default-resource-principal>
      </resource-ref>
    <resource-ref>
        <res-ref-name>TheMailSession</res-ref-name>
        <jndi-name>mail/MySession</jndi-name>
    </resource-ref>
  <jsp-config/>
</sun-web-app>

