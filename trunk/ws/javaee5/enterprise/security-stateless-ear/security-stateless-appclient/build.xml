<project name="security-stateless-appclient" default="run-app-client" basedir=".">

    <property name="is.app-client.module" value="true"/>

    <import file="../../../../bp-project/main.xml"/>

    <target name="run-app-client">
        <antcall target="bpp-run-secure-app-client">
            <param name="app-client.jar" value="${app.client.jar}"/>
            <param name="file.realm.user.id" value="javaee"/>
            <param name="file.realm.user.password" value="javaee"/>
        </antcall>
    </target>

</project>
