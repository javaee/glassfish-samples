<project name="locking" default="all" basedir=".">

    <property name="is.war.module" value="true"/>

    <import file="../../../bp-project/main.xml"/>


    <target name="all" depends="default,deploy">
        <antcall target="run"/>
    </target>

    <target name="-pre-clean" depends="undeploy">
        <subant target="clean">
            <fileset dir="locking_client" includes="build.xml"/>
        </subant>
    </target>

    <target name="run">
        <subant target="run">
            <fileset dir="locking_client" includes="build.xml"/>
        </subant>
    </target>
</project>
